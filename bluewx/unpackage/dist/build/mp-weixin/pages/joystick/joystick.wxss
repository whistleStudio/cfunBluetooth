.joystick{background:url(https://dict.cfunworld.com/img/public/joystickbg.png) center/cover no-repeat;box-sizing:border-box;position:relative}.joystick .axis{width:160px;height:160px;background-color:rgba(255,255,255,.05);border:1px solid rgba(255,255,255,.15);border-radius:50%;position:absolute;left:50px;top:40px}.joystick .axis .axis-center{width:70px;height:70px;border-radius:50%;background:url(data:image/png;base64,) right center/contain no-repeat;z-index:9}.joystick .axis .arrow{position:absolute;width:198px;height:198px;background:url(data:image/png;base64,) center/contain no-repeat;left:50%;top:50%;z-index:2}.joystick .axis .dir{width:138px;height:138px;position:absolute;left:50%;top:50%;transform:translate(-50%,-50%);background:url(data:image/png;base64,) center/contain no-repeat}.joystick .btn-group{position:absolute;bottom:5px;left:50px;width:160px;height:220px;z-index:10}.joystick .btn-group .btn{transform:rotate(90deg);border-radius:50%;position:absolute;text-align:center;background-image:linear-gradient(-45deg,#1d2937,#46768b);color:rgba(255,255,255,.8);border:2px solid rgba(200,200,230,.1);opacity:.6}.joystick .btn-group .btn:first-of-type{line-height:70px;bottom:40px;width:70px;height:70px;font-size:25px}.joystick .btn-group .btn:not(:first-of-type){width:55px;height:55px;line-height:55px}.joystick .btn-group .btn:nth-of-type(3){left:65px;bottom:120px}.joystick .btn-group .btn:last-of-type{right:0;bottom:50px}.joystick .btn-group .btnAct{opacity:1}
